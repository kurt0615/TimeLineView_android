<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="8dip" />
            <stroke android:width="1dip" android:color="#FF3488FD" />
            <gradient android:startColor="#FF3488FD" android:endColor="#FF3488FD" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="8dip" />
            <stroke android:width="1dip" android:color="#FF34ADFD" />
            <gradient android:startColor="#FF34ADFD" android:endColor="#FF34ADFD" />
        </shape>
    </item>
</selector>